.strip__dark {
  h3.topic__text {
    margin-bottom: 0.5rem;
    margin-top: 1.5rem;

    em {
      &:before {
        @include icon__back;
        content: " ";
        display: inline-block;
        margin: 0 10px 0 0;
      }
    }
  }
}

.topic__preview {
  h1.topic__text {
    @include h3-flood;
    color: $green-dark;
    a {
      color: $green-dark;
    }
  }

}
